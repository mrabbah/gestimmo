                                            <listheader label="Projet "/> 
                                            <listheader label="Client "/>
                                            <listheader label="Date exécution"/>
                                            <listheader label="Date déclenchement "/>
                                            <listheader label="Contenu "/>
                                        </listhead>
                                       
                                       
                                        <listitem self="@{each=element}"> 
                                            <listcell label="@{element.projet}" />
                                            <listcell label="@{element.client}" />
                                            <listcell>
                                                <datebox style="color:red" value="@{element.date}" width="90%" format="dd/MM/yyyy" buttonVisible="false" readonly="true"/>
                                            </listcell>
                                            <listcell>
                                                <datebox style="color:red" value="@{element.dateDeclenchement}" width="90%" format="dd/MM/yyyy" buttonVisible="false" readonly="true"/>
