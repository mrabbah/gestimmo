                                            <grid>
                                                <rows>
                                                    <row>
                                                        <cell width= "80px">
                                                            <label value="Projet"/>
                                                        </cell>	
                                                        <cell >
                                                            <combobox id="coprojets" hflex="1" model="@{tdbWin.projets}" selectedItem="@{tdbWin.projetSelected}" onSelect="tdbWin.genererChart(true)" >
                                                                <comboitem self="@{each=elementprojet}" label="@{elementprojet}"/>
                                                            </combobox>
                                                        </cell>
                                                    </row>
                                                </rows>
                                            </grid>
                                        </panelchildren>                                                                                                                      
                                    </panel>
                                    <separator/>
                                    <chart id="etatprojet" title="Etat des biens du projet" width="448" height="210"
