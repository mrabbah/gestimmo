                                            </listcell>
                                        </listitem>
                                       
                                    </listbox>
<!--                                    <button id="btnAlrt" onClick="tdbWin.alerteMail()" label="mail envoi" image="/images/skin/cancel.png"/>-->
                                </panelchildren>
                            </panel>
