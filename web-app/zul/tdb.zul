                                            <listheader label="Projet "/> 
                                            <listheader label="Client "/>
                                            <listheader label="Date paiement"/>
                                            <listheader label="Date encaissement"/>
                                            <listheader label="Montant paiement"/>
                                        </listhead>
                                        
                                        <listitem self="@{each=element}"> 
                                            <listcell label="@{element.phase.projet}" style="color:red" />
                                            <listcell label="@{element.phase.client}" style="color:red" />
