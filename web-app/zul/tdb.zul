                                            <listcell>
                                                <datebox style="color:red" value="@{element.dateEncaissement}" width="90%" format="dd/MM/yyyy" buttonVisible="false" readonly="true"/>
                                            </listcell>
                                            <listcell>
                                                <doublebox value="@{element.montant}" hflex="1" format="#,##0.##" style="color:red" readonly="true" />
                                            </listcell>
                                        </listitem>
                                        
